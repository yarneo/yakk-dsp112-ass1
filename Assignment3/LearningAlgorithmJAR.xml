<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="makejar" name="LearningAlgorithm JAR">
	<target name="makejar" description="Create a JAR for the project">
		<jar jarfile="learningalgo.jar" filesetmanifest="mergewithoutmain">
			<manifest>
				<attribute name="Main-Class" value="MapReduce.LearningAlgorithm" />
				<attribute name="Class-Path" value="." />
			</manifest>
			<fileset dir="bin" />
			<zipfileset excludes="META-INF/*.SF" src="dsp-tagger.jar" />
		</jar>
	</target>
</project>
